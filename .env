NODE_ENV=development
PORT=9099
JWT_SECRET=your_jwt_secret_key_here_change_in_production
JWT_EXPIRES_IN=7d

# MongoDB Connection
# Option 1: Direct URI (for local development without authentication)
# MONGODB_URI=mongodb://localhost:27017/voting-app

# Option 2: Individual components with authentication (uncomment and use these if you need authentication)
MONGODB_HOST=localhost
MONGODB_PORT=27017
MONGODB_DATABASE=voting-app
MONGODB_USERNAME=root
MONGODB_PASSWORD=example
MONGODB_AUTH_DATABASE=admin